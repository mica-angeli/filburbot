<?xml version="1.0"?>
<launch>
  <node
    pkg="filburbot_base"
    type="filburbot_base_node"
    name="filburbot_node" />

  <node
    pkg="rosserial_python"
    type="serial_node.py"
    name="serial_node"
    output="screen">
    <param name="port" value="/dev/ttyACM0" />
    <param name="baud" value="57600" />
  </node>

  <include file="$(find filburbot_control)/launch/control.launch" />

</launch>
